@article{Shen2012,
abstract = {The phenotypic effect of a gene is normally described by the mean-difference between alternative genotypes. A gene may, however, also influence the phenotype by causing a difference in variance between genotypes. Here, we reanalyze a publicly available Arabidopsis thaliana dataset [1] and show that genetic variance heterogeneity appears to be as common as normal additive effects on a genomewide scale. The study also develops theory to estimate the contributions of variance differences between genotypes to the phenotypic variance, and this is used to show that individual loci can explain more than 20{\%} of the phenotypic variance. Two well-studied systems, cellular control of molybdenum level by the ion-transporter MOT1 and flowering-time regulation by the FRI-FLC expression network, and a novel association for Leaf serration are used to illustrate the contribution of major individual loci, expression pathways, and gene-by-environment interactions to the genetic variance heterogeneity.},
author = {Shen, Xia and Pettersson, Mats and Ronnegard, Lars and Carlborg, Orjan},
doi = {10.1371/journal.pgen.1002839},
file = {:Users/rcorty/Library/Application Support/Mendeley Desktop/Downloaded/Shen et al. - 2012 - Inheritance beyond plain heritability variance-controlling genes in Arabidopsis thaliana.pdf:pdf;:Users/rcorty/Library/Application Support/Mendeley Desktop/Downloaded/Shen et al. - 2012 - Inheritance beyond plain heritability variance-controlling genes in Arabidopsis thaliana.docx:docx},
issn = {1553-7404},
journal = {PLoS Genet.},
keywords = {Anion Transport Proteins,Anion Transport Proteins: genetics,Anion Transport Proteins: metabolism,Arabidopsis,Arabidopsis Proteins,Arabidopsis Proteins: genetics,Arabidopsis Proteins: metabolism,Arabidopsis: genetics,Arabidopsis: metabolism,Base Sequence,Databases,Flowers,Flowers: genetics,Flowers: metabolism,Gene Expression Regulation,Genes,Genetic,Genetic Association Studies,Genetic Loci,Genetic Variation,Genotype,Heritable,MADS Domain Proteins,MADS Domain Proteins: genetics,MADS Domain Proteins: metabolism,Molecular Sequence Data,Molybdenum,Molybdenum: metabolism,Phenotype,Plant,Plant Leaves,Plant Leaves: genetics,Plant Leaves: metabolism,Quantitative Trait,Systems Biology},
number = {8},
