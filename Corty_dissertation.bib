issn = {1943-2631},
journal = {Genetics},
keywords = {complex traits,e,effectively in fi nite,effects,falconer,fi tness,genetics of environmental variance,i,is assumed to be,mass selection,n the classic model,number,of genes and environmental,of quantitative genetics,optimum trait,p ¼ g þ,the phenotype,the sum of an},
number = {4},
pages = {1255--69},
pmid = {25631318},
title = {{Heritable environmental variance causes nonlinear relationships between traits: application to birth weight and stillbirth of pigs.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25631318},
volume = {199},
year = {2015}
}
