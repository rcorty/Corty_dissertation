@article{Simon2013,
abstract = {BACKGROUND: The mouse inbred line C57BL/6J is widely used in mouse genetics and its genome has been incorporated into many genetic reference populations. More recently large initiatives such as the International Knockout Mouse Consortium (IKMC) are using the C57BL/6N mouse strain to generate null alleles for all mouse genes. Hence both strains are now widely used in mouse genetics studies. Here we perform a comprehensive genomic and phenotypic analysis of the two strains to identify differences that may influence their underlying genetic mechanisms.$\backslash$n$\backslash$nRESULTS: We undertake genome sequence comparisons of C57BL/6J and C57BL/6N to identify SNPs, indels and structural variants, with a focus on identifying all coding variants. We annotate 34 SNPs and 2 indels that distinguish C57BL/6J and C57BL/6N coding sequences, as well as 15 structural variants that overlap a gene. In parallel we assess the comparative phenotypes of the two inbred lines utilizing the EMPReSSslim phenotyping pipeline, a broad based assessment encompassing diverse biological systems. We perform additional secondary phenotyping assessments to explore other phenotype domains and to elaborate phenotype differences identified in the primary assessment. We uncover significant phenotypic differences between the two lines, replicated across multiple centers, in a number of physiological, biochemical and behavioral systems.$\backslash$n$\backslash$nCONCLUSIONS: Comparison of C57BL/6J and C57BL/6N demonstrates a range of phenotypic differences that have the potential to impact upon penetrance and expressivity of mutational effects in these strains. Moreover, the sequence variants we identify provide a set of candidate genes for the phenotypic differences observed between the two strains.},
author = {Simon, Michelle M. and Greenaway, Simon and White, Jacqueline K. and Fuchs, Helmut and Gailus-Durner, Val{\'{e}}rie and Wells, Sara and Sorg, Tania and Wong, Kim and Bedu, Elodie and Cartwright, Elizabeth J. and Dacquin, Romain and Djebali, Sophia and Estabel, Jeanne and Graw, Jochen and Ingham, Neil J. and Jackson, Ian J. and Lengeling, Andreas and Mandillo, Silvia and Marve, Jacqueline and Meziane, Hamid and Preitner, Fr{\'{e}}d{\'{e}}ric and Puk, Oliver and Roux, Michel and Adams, David J. and Atkins, Sarah and Ayadi, Abdel and Becker, Lore and Blake, Andrew and Brooker, Debra and Cater, Heather and Champy, Marie France and Combe, Roy and Danecek, Petr and {Di Fenza}, Armida and Gates, Hilary and Gerdin, Anna Karin and Golini, Elisabetta and Hancock, John M. and Hans, Wolfgang and H{\"{o}}lter, Sabine M. and Hough, Tertius and Jurdic, Pierre and Keane, Thomas M. and Morgan, Hugh and M{\"{u}}ller, Werner and Neff, Frauke and Nicholson, George and Pasche, Bastian and Roberson, Laura Anne and Rozman, Jan and Sanderson, Mark and Santos, Luis and Selloum, Mohammed and Shannon, Carl and Southwel, Anne and Tocchini-Valentini, Glauco P. and Vancollie, Valerie E. and Westerberg, Henrik and Wurst, Wolfgang and Zi, Min and Yalcin, Binnaz and Ramirez-Solis, Ramiro and Steel, Karen P. and Mallon, Ann Marie and {De Angelis}, Martin Hrabe and Herault, Yann and Brown, Steve D.M.},
